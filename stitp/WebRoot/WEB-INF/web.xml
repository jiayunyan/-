<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>stitp</display-name>
  <servlet>
    <description>保存学生信息</description>
    <display-name>SaveStudent</display-name>
    <servlet-name>SaveStudent</servlet-name>
    <servlet-class>com.yjy.server.SaveStudent</servlet-class>
  </servlet>
  <servlet>
    <description>保存老师信息</description>
    <display-name>SaveTeacher</display-name>
    <servlet-name>SaveTeacher</servlet-name>
    <servlet-class>com.yjy.server.SaveTeacher</servlet-class>
  </servlet>
  <servlet>
    <description>保存项目信息</description>
    <display-name>SaveProject</display-name>
    <servlet-name>SaveProject</servlet-name>
    <servlet-class>com.yjy.server.SaveProject</servlet-class>
  </servlet>
  <servlet>
    <description>更新学生信息</description>
    <display-name>UpdateStudent</display-name>
    <servlet-name>UpdateStudent</servlet-name>
    <servlet-class>com.yjy.server.UpdateStudent</servlet-class>
  </servlet>
  <servlet>
    <description>更新老师信息</description>
    <display-name>UpdateTeacher</display-name>
    <servlet-name>UpdateTeacher</servlet-name>
    <servlet-class>com.yjy.server.UpdateTeacher</servlet-class>
  </servlet>
  <servlet>
    <description>删除学生</description>
    <display-name>RemoveStudent</display-name>
    <servlet-name>RemoveStudent</servlet-name>
    <servlet-class>com.yjy.server.RemoveStudent</servlet-class>
  </servlet>
  <servlet>
    <description>删除学生</description>
    <display-name>RemoveStudent</display-name>
    <servlet-name>RemoveTeacher</servlet-name>
    <servlet-class>com.yjy.server.RemoveTeacher</servlet-class>
  </servlet>







  <servlet-mapping>
    <servlet-name>SaveStudent</servlet-name>
    <url-pattern>/SaveStudent</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SaveTeacher</servlet-name>
    <url-pattern>/SaveTeacher</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SaveProject</servlet-name>
    <url-pattern>/SaveProject</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UpdateStudent</servlet-name>
    <url-pattern>/UpdateStudent</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UpdateTeacher</servlet-name>
    <url-pattern>/UpdateTeacher</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RemoveStudent</servlet-name>
    <url-pattern>/RemoveStudent</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RemoveTeacher</servlet-name>
    <url-pattern>/RemoveTeacher</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
</web-app>